#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i;
    cout<<"\nenter value of n\n";
    cin>>n;
    char a[n];
    string s ="";
    cout<<"\nenter value of an array\n";
    for(i=0;i<n;i++)
    {
        cin>>a[i];
        s += a[i];
    }
    cout<<s;
    return 0;
}
